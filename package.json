{
  "name": "taiwanlent-backend-project",
  "version": "1.0.0",
  "description": "taiwanlent backend project",
  "main": "index.js",
  "scripts": {
    "startlocalhost": "nodemon --exec babel-node index.js",
    "start": "node ./node_modules/pm2/bin/pm2 start index.js --name taiwanlent-backend",
    "startwithoutPM2": "node index.js",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs",
    "startSub": "nodemon --exec babel-node ./DBscripts/index.js",
    "test": "jest",
    "startNewDB": "docker build --no-cache --tag=\"taiwanlent_postgresql\" ./DBscripts/PostgreSQL/ && (docker rm -f postgresdb  ||  echo 'no container named postgresdb') && docker run --name postgresdb  -e POSTGRES_PASSWORD=1234 -h postgres -p 5432:5432 -d taiwanlent_postgresql && docker logs postgresdb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julian-Chu/taiwanlent-backend-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Julian-Chu/taiwanlent-backend-project/issues"
  },
  "homepage": "https://github.com/Julian-Chu/taiwanlent-backend-project#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "jest": "^22.4.3",
    "nodemon": "^1.18.2",
    "sinon": "^5.0.7",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "aws-sdk": "^2.297.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.4.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jwt-simple": "^0.5.1",
    "mysql2": "^1.5.3",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "pm2": "^3.0.4",
    "sendgrid": "^5.2.3",
    "sequelize": "^4.37.7",
    "sequelize-cli": "^4.0.0"
  }
}
